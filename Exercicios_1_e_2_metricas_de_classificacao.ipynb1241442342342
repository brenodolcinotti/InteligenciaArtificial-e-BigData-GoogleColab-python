    {
      "cell_type": "code",
      "source": [
        "# Separando variáveis preditoras e alvo\n",
        "X = dados.drop(\"aprovado\", axis=1)\n",
        "y = dados[\"aprovado\"]\n",
        "\n",
        "# Separando treino e teste\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.3, random_state=42)\n",
        "\n",
        "# Pré-processamento (Codificar variáveis categóricas)\n",
        "cat_features = [\"tipo_material\", \"turno\"]\n",
        "num_features = [\"temperatura\", \"pressao\", \"tempo_operacao\"]\n",
        "\n",
        "# OneHotEncoder para as categorias\n",
        "preprocessador = ColumnTransformer(\n",
        "    transformers=[\n",
        "        (\"cat\", OneHotEncoder(drop=\"first\"), cat_features),\n",
        "        (\"num\", \"passthrough\", num_features)\n",
        "    ]\n",
        ")\n",
        "\n",
        "# Modelos\n",
        "\n",
        "# Regressão Logística\n",
        "modelo_lr = Pipeline(steps=[\n",
        "    (\"preprocess\", preprocessador),\n",
        "    (\"classificador\", LogisticRegression(max_iter=1000))\n",
        "])\n",
        "\n",
        "# Random Forest\n",
        "modelo_rf = Pipeline(steps=[\n",
        "    (\"preprocess\", preprocessador),\n",
        "    (\"classificador\", RandomForestClassifier(n_estimators=100, random_state=42))\n",
        "])\n",
        "\n",
        "# Treinamento dos modelos\n",
        "modelo_lr.fit(X_train, y_train)\n",
        "modelo_rf.fit(X_train, y_train)\n",
        "\n",
        "# Predições\n",
        "y_pred_lr = modelo_lr.predict(X_test)\n",
        "y_pred_rf = modelo_rf.predict(X_test)\n",
        "\n",
        "# Avaliação\n",
        "def avaliar_modelo(y_true, y_pred, nome):\n",
        "    print(f\"\\n===== {nome} =====\")\n",
        "    print(f\"Acurácia:  {accuracy_score(y_true, y_pred):.3f}\")\n",
        "    print(f\"Precisão:  {precision_score(y_true, y_pred):.3f}\")\n",
        "    print(f\"Recall:    {recall_score(y_true, y_pred):.3f}\")\n",
        "    print(f\"F1-Score:  {f1_score(y_true, y_pred):.3f}\")\n",
        "\n",
        "avaliar_modelo(y_test, y_pred_lr, \"Regressão Logística\")\n",
        "avaliar_modelo(y_test, y_pred_rf, \"Random Forest\")\n",
        "\n",
        "# Relatório detalhado\n",
        "print(\"\\nRelatório - Regressão Logística\")\n",
        "print(classification_report(y_test, y_pred_lr))\n",
        "\n",
        "print(\"\\nRelatório - Random Forest\")\n",
        "print(classification_report(y_test, y_pred_rf))"
      ],
